<interface>
    <template class="GtkListItem">
        <property name="child">
            <object class="GtkTreeExpander" id="expander">
                <binding name="list-row">
                    <lookup name="item">GtkListItem</lookup>
                </binding>
                <property name="child">
                    <object class="GtkLabel">
                        <binding name="label">
                            <closure type="gchararray" function="get_file_name">
                                <lookup name="item">GtkListItem</lookup>
                            </closure>
                        </binding>
                    </object>

                </property>
            </object>
        </property>
    </template>
</interface>